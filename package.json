{
  "name": "gulp4-webpack",
  "version": "1.0.0",
  "description": "A modern ES6 Gulp workflow combined with Webpack",
  "scripts": {
    "dev": "gulp --require babel-register --gulpfile tasks",
    "build": "SET NODE_ENV=production & gulp build --require babel-register --gulpfile tasks",
    "build-orig": "NODE_ENV=production gulp build --require babel-register --gulpfile tasks"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "babelrc": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pascalaoms/gulp4-webpack"
  },
  "author": "Pascal Klau",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.24.6",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3"
  }
}
